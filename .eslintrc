
{
  "root": true,
  "ignorePatterns": ["node_modules/"],
  "env": {
    "es2020": true,
    "browser": true,
    "node": true
  },
  "globals": {
    "chrome": true
  },
  "rules": {
    "no-console": 0,
    "no-empty": [1, {"allowEmptyCatch": true}]
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "createDefaultProgram": true
  }
}

